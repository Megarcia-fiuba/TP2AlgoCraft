<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="algocraft.controller.VistaHerramientasController">
    <children>
        <BorderPane fx:id="borderPane" prefHeight="100.0" prefWidth="1000.0">
            <center>
                <GridPane fx:id="grillaHerramienta" gridLinesVisible="true" prefHeight="300.0" prefWidth="300.0" BorderPane.alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                </GridPane>
            </center>
            <top>
                <MenuBar prefHeight="34.0" prefWidth="790.0" BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="Archivo">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Salir" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Ayuda">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Acerca de..." />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <left>
                <VBox prefHeight="381.0" prefWidth="152.0" BorderPane.alignment="BOTTOM_LEFT">
                    <children>
                        <Button id= "botonRegresar" fx:id="botonRegresar" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleAccionBotonRegresar" text="Regresar" textAlignment="CENTER" textOverrun="CLIP" />
                    </children>
                    <cursor>
                        <Cursor fx:constant="DEFAULT" />
                    </cursor>
                    <BorderPane.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </BorderPane.margin>
                </VBox>
            </left>
        </BorderPane>
    </children>
</AnchorPane>